<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<!-- mvn mybatis-generator:generate -Dmybatis.generator.overwrite=true -->
<generatorConfiguration>

    <context id="mysqlgenerator" targetRuntime="MyBatis3" defaultModelType="flat">
        <property name="autoDelimitKeywords" value="true"/>
        <!--可以使用``包括字段名，避免字段名与sql保留字冲突报错-->
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
        <property name="javaFileEncoding" value="UTF-8"/>

        <!-- 自动生成toString方法 -->
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin"/>
        <!-- 自动生成equals方法和hashcode方法 -->
        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin"/>

        <!-- 注释  这里千万别改 -->
        <commentGenerator>
            <!-- 是否生成的实体类显示注释 -->
            <property name="suppressAllComments" value="false"/>
            <!-- 是否生成注释带时间戳 -->
            <property name="suppressDate" value="true"/>
            <!-- 是否生成注释带数据库的备注 -->
            <property name="addRemarkComments" value="true"/>
        </commentGenerator>

        <!--数据库连接信息-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://121.36.77.21:5019/dfq?useUnicode=true&amp;characterEncoding=UTF-8&amp;serverTimezone=UTC&amp;verifyServerCertificate=false&amp;useSSL=false"
                        userId="root"
                        password="dfq5019."/>


        <javaModelGenerator targetPackage="com.itdfq.upload.entity" targetProject="src/main/java"/>
        <sqlMapGenerator targetPackage="com.itdfq.upload.dao" targetProject="src/main/resources" />
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.itdfq.upload.dao"
                             targetProject="src/main/java"/>
        <!--表名-->
        <table tableName="upload">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>


    </context>
</generatorConfiguration>
